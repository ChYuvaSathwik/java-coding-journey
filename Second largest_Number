package com.sathwik.first;
import java.util.*;
public class FirstProgram{
	public static void main(String [] args) {
		Scanner sc=new Scanner(System.in);
		int sathwik=sc.nextInt();
		int [] balu=new int[sathwik];
		for(int i=0;i<sathwik;i++) {
			balu[i]=sc.nextInt();
		}
		Arrays.sort(balu);
		int k=balu.length-2;
		System.out.println(balu[k]);
	}
	}
