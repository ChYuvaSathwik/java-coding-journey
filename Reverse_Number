package com.sathwik.first;
import java.util.*;
public class FirstProgram{
	public static void main(String [] args) {
		Scanner sc=new Scanner(System.in);
		int sathwik=sc.nextInt();
		String Balu="";
		String Praneeth=Integer.toString(sathwik);
		for(int i=Praneeth.length()-1;i>=0;i--) {
			Balu=Balu+Praneeth.charAt(i);
		}
		System.out.println(Balu);
	}
	}
