package com.sathwik.first;

import java.util.*;

public class FirstProgram {
    public static void main(String[] args) {
    	Scanner sc=new Scanner(System.in);
    	String sathwik=sc.next();
    	int count=0;
    	for(int i=0;i<sathwik.length();i++) {
    		for(int j=i+1;j<sathwik.length()+1;j++) {
    			String Praneeth=sathwik.substring(i,j);
    			count=count+1;
    			
    		}
    	}
    	System.out.println(count);
}}
