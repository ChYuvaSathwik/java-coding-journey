package com.sathwik.first;

import java.util.*;

public class FirstProgram {
    public static void main(String[] args) {
    	Scanner sc=new Scanner(System.in);
    	String sathwik=sc.next();
    	int Praneeth=sc.nextInt();
    	for(int i=0;i<sathwik.length()-Praneeth+1;i++) {
    		String k_substrings=sathwik.substring(i,Praneeth+i);
    		System.out.println(k_substrings);
    		
    	}
    		
    	}
    	
}
