package com.sathwik.first;

import java.util.*;

public class FirstProgram {
    public static void main(String[] args) {
    	Scanner sc=new Scanner(System.in);
    	int sathwik=sc.nextInt();
    	int [] Praneeth= new int[sathwik];
    	for(int i=0;i<sathwik;i++) {
    		Praneeth[i]=sc.nextInt();
    	}
    	Arrays.sort(Praneeth);
    	int balu=1;
    	for(int j=Praneeth.length-1;j>=2;j--) {
    		balu=balu*Praneeth[j];
    		
    	}
    	System.out.println(balu);
}}
