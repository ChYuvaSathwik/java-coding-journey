package com.sathwik.first;
import java.util.*;
public class FirstProgram{
	public static void main(String [] args) {
		Scanner sc=new Scanner(System.in);
		int sathwik=sc.nextInt();
		int [] balu=new int[sathwik];
		int max_value=Integer.MIN_VALUE;
		for(int i=0;i<sathwik;i++) {
			balu[i]=sc.nextInt();
		}
		for(int j=0;j<sathwik;j++) {
			if (balu[j]>max_value) {
				max_value=balu[j];
			}
		}
		System.out.println(max_value);
	}
	}
