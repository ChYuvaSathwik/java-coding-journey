package com.sathwik.first;
import java.util.*;
public class FirstProgram{
	public static void main(String [] args ) {
		Scanner sc=new Scanner(System.in);
		String sathwik=sc.nextLine();
		String[] Balu=sathwik.split(" ");
		ArrayList<String>hm=new ArrayList<>();
		for(String s:Balu) {
			char[] Jasanth=s.toCharArray();
			Arrays.sort(Jasanth);
			String back_to_string=new String(Jasanth);
			hm.add(back_to_string);
			}
		boolean Praneeth=false;
		String mouse=hm.get(0);
		for(String new_1:hm) {
			if(new_1.equals(mouse)) {
				Praneeth=true;
			}
			else {
				Praneeth=false;
				break;
			}}
		if(Praneeth) {
			System.out.println("Anagram");
			
		}
		else {
			System.out.println("Not Anagram");
		}
		
	}}
	
