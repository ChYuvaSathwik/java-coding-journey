package com.sathwik.first;

import java.util.*;

public class FirstProgram {
    public static void main(String[] args) {
    	Scanner sc=new Scanner(System.in);
    	String sathwik=sc.next();
    	String p="aeiouAEIOU";
    	int count=0;
    	for(int i=0;i<sathwik.length();i++) {
    		char h=sathwik.charAt(i);
    		if(p.indexOf(h)!=-1) {
    			count++;
    		}
    	}
    		System.out.println(count);
    	
    	}
    	
}
