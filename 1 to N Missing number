package com.sathwik.first;
import java.util.*;
public class FirstProgram{
	public static void main(String [] args ) {
		Scanner sc=new Scanner(System.in);
		int sathwik =sc.nextInt();
		int [] Praneeth=new int[sathwik-1];
		int Balu=0;
		for(int i=0;i<sathwik-1;i++) {
			Praneeth[i]=sc.nextInt();
			
		}
		for(int j=0;j<sathwik-1;j++) {
			Balu=Balu+Praneeth[j];
		}
		int c=sathwik*(sathwik+1)/2;
		System.out.println(c-Balu);
		
	}
	}
