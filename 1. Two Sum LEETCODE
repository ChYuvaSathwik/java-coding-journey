class Solution {
    public int[] twoSum(int[] nums, int target){
        ArrayList<Integer>hm=new ArrayList<>();
        for(int i=0;i<nums.length;i++){
            for(int j=0;j<nums.length;j++){
                if(i!=j){
                    if (nums[i]+nums[j]==target){
                        hm.add(i);
                        hm.add(j);
                        break;
                    }
                }

            }}
            HashSet<Integer>ok=new HashSet<>(hm);
            ArrayList<Integer>notok=new ArrayList<>(ok);
            int sathwik=notok.size();
            int [] Praneeth=new int[sathwik];
            for(int k=0;k<sathwik;k++){
                Praneeth[k]=notok.get(k);
            }
            return Praneeth;
    }}




class Solution {
    public int[] twoSum(int[] nums, int target) {
        for (int i = 0; i < nums.length; i++) {
            for (int j = i + 1; j < nums.length; j++) {
                if (nums[i] + nums[j] == target) {
                    return new int[]{i, j};
                }
            }
        }
        return new int[]{};
    }
}
