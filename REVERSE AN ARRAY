package com.sathwik.first;
import java.util.*;
public class FirstProgram{
	public static void main(String [] args) {
		Scanner sc=new Scanner(System.in);
		int sathwik=sc.nextInt();
		int [] balu=new int[sathwik];
		int [] jasanth=new int[sathwik];
		int k=0;
		for(int i=0;i<sathwik;i++) {
			balu[i]=sc.nextInt();
		}
		for(int j=balu.length-1;j>=0;j--) {
			if(k<=sathwik) {
				jasanth[k]=balu[j];
				k=k+1;
			}
		}
		System.out.println(Arrays.toString(jasanth));
		
	}
	}
